<?xml version="1.0" encoding="UTF-8"?>

<configuration>

	<property name="APP_NAME" value="schemacrawler-additionnallints" />
	
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<layout class="ch.qos.logback.classic.PatternLayout">
			<Pattern>[${APP_NAME}] %d{yyyy-MM-dd HH:mm:ss} [%thread] %-5level %logger{36} - %msg%n</Pattern>
		</layout>
	</appender>

    <logger name="io.github.mbarre.schemacrawler" level="debug" />
    <!--<logger name="org.springframework.security" level="DEBUG" />-->
    <!--<logger name="org.zkoss" level="DEBUG" />-->
    <!--<logger name="org.hibernate" level="TRACE" />-->


	<root level="info">
		<appender-ref ref="STDOUT" />
	</root>

</configuration>